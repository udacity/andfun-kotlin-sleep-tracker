<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/barchartFragment">

    <fragment
        android:id="@+id/barchartFragment"
        android:name="com.example.android.sleeptracker.BarChartFragment"
        android:label="fragment_barchart"
        tools:layout="@layout/fragment_barchart">
        <action
            android:id="@+id/action_barchartFragment_to_detailFragment"
            app:destination="@+id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.android.sleeptracker.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" />

</navigation>
